    "cross-env": "^6.0.3",
    "electron": "6.1.5",
    "eslint": "^6.7.2",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
